{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "declaration": true,
      "sourceMap": true,
      "jsx": "react",
      "jsxFactory": "ui.h",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "outDir": "site/built/js",
      "lib": [
          "es6",
          "dom"
      ]
    },

    "include": ["src/**/*"],
    "exclude": ["src/less/**/*"]
}